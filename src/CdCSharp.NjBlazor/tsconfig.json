{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "ES2022",
    "target": "es6",
    "outDir": "./Types/js",
    "moduleResolution": "node",
    "sourceMap": true,
    "typeRoots": [
      "node_modules/@types",
      "./@types"
    ],
    "paths": {
    }
  },
  "exclude": [
    "node_modules",
    "./Types/js"
  ],
  "include": [
    "./Types/**.ts",
    "./Types/**/*.ts"
  ]
}